{
  "name": "backend_poc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "start": "npm run serve",
    "build": "npm run lint && npm run build-ts",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest  --forceExit --verbose --runInBand --coverage",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migrate": "typeorm migration:run",
    "migrate:run": "node_modules/.bin/typeorm migration:run",
    "dev": "pm2-dev server.js",
    "migration:create": "npm run typeorm -- migration:create -d src/database/migration -n",
    "rollback": "npm run typeorm migration:revert"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgjatin/backend_poc.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sgjatin/backend_poc/issues"
  },
  "homepage": "https://github.com/sgjatin/backend_poc#readme",
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^17.0.35",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "concurrently": "^5.3.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.20",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "@types/jest": "^29.2.5",
    "@types/supertest": "^2.0.12",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "i18n": "^0.15.1",
    "jest": "^29.3.1",
    "jest-express": "^1.12.0",
    "nodemailer": "^6.9.1",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "path": "^0.12.7",
    "pg": "^8.5.1",
    "save": "^2.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "tslib": "^1.11.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.29",
    "typeorm-naming-strategies": "^4.1.0",
    "uuidv4": "^6.2.13"
  },
  "resolutions": {
    "tslib": "1.11.2"
  }
}
